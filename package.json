{
  "name": "@wmde/query-builder",
  "version": "1.0.0",
  "private": true,
  "description": "A simple query builder for Wikidata SPARQL queries",
  "author": {
    "name": "The Wikidata team"
  },
  "scripts": {
    "e2e": "vue-cli-service test:e2e",
    "e2e:saucelabs": "env DATE=\"$(date)\" nightwatch --config nightwatch.conf.js --env sauceChrome,sauceFirefox,sauceEdge,sauceSafari",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --mode production",
    "test:unit": "vue-cli-service test:unit",
    "test:lint": "vue-cli-service lint --no-fix --max-warning 0",
    "test": "npm-run-all test:* && vue-cli-service build",
    "test:lintcss": "stylelint '**/*.vue'",
    "test:a11y": "jest --testMatch=\"**/tests/a11y/**\"",
    "fix:css": "stylelint '**/*.vue' --fix",
    "fix:ts": "vue-cli-service lint",
    "netlify": "npx netlify deploy --dir=dist --json"
  },
  "main": "index.js",
  "dependencies": {
    "@wmde/wikit-tokens": "^2.0.0-alpha.2",
    "@wmde/wikit-vue-components": "^2.0.0-alpha.2",
    "core-js": "^3.6.5",
    "lodash.debounce": "^4.0.8",
    "ress": "^3.0.0",
    "saucelabs": "^4.5.2",
    "sparqljs": "^3.0.3",
    "vue": "^2.6.11",
    "vue-banana-i18n": "^1.3.0",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.19",
    "@types/lodash": "^4.14.167",
    "@types/node": "^14.14.9",
    "@types/jest-axe": "^3.5.0",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-e2e-nightwatch": "^4.5.8",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "^1.0.3",
    "@wmde/eslint-config-wikimedia-typescript": "^0.2.0",
    "chromedriver": "84",
    "eslint": "^7.6.0",
    "eslint-config-wikimedia": "^0.17.0",
    "eslint-plugin-vue": "^6.2.2",
    "geckodriver": "^1.20.0",
    "netlify-cli": "^2.68.4",
    "jest-axe": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-dir-pseudo-class": "^5.0.0",
    "postcss-logical": "^4.0.2",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "stylelint": "^13.7.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-use-logical": "^1.1.0",
    "typescript": "~3.9.3",
    "vue-template-compiler": "^2.6.11"
  },
  "_id": "@wmde/query-builder@1.0.0",
  "bugs": {
    "url": "https://phabricator.wikimedia.org/tag/wikidata_query_builder/"
  },
  "homepage": "https://github.com/wmde/query-builder#readme",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wmde/query-builder.git"
  }
}
