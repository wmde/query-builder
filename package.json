{
  "name": "@wmde/query-builder",
  "version": "1.0.0",
  "private": true,
  "description": "A query builder for Wikidata SPARQL queries",
  "author": {
    "name": "The Wikidata team"
  },
  "scripts": {
    "cypress": "CYPRESS_API_URL=${VUE_APP_WIKIBASE_API_URL} CYPRESS_BASE_URL=${CYPRESS_BASE_URL:-http://localhost:8080} cypress run --browser chromium --headless",
    "test:lint-cypress": "vue-cli-service lint --no-fix --max-warnings 0 cypress",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --mode production",
    "test:unit": "vue-cli-service test:unit",
    "test:integration": "vue-cli-service test:unit -c jest.config.integration.js",
    "test:lint": "vue-cli-service lint --no-fix --max-warning 0",
    "test": "npm-run-all test:* && vue-cli-service build",
    "test:lintcss": "stylelint '**/*.vue'",
    "test:a11y": "jest --testMatch=\"**/tests/a11y/**\"",
    "test:cy": "env $(grep -v '^#' .env.example | xargs) start-server-and-test serve http-get://localhost:8080 cypress",
    "fix:css": "stylelint '**/*.vue' --fix",
    "fix:ts": "vue-cli-service lint",
    "fix:cypress": "vue-cli-service lint cypress",
    "netlify": "npx netlify deploy --dir=dist --json"
  },
  "main": "index.js",
  "dependencies": {
    "@wmde/wikit-tokens": "^2.0.0-alpha.10",
    "@wmde/wikit-vue-components": "^2.0.0-alpha.10",
    "core-js": "^3.8.3",
    "lodash.debounce": "^4.0.8",
    "ress": "^3.0.0",
    "sparqljs": "^3.2.0",
    "vue": "^2.6.11",
    "vue-banana-i18n": "^1.3.0",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@types/jest": "^24.0.19",
    "@types/jest-axe": "^3.5.1",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.28",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli-plugin-babel": "^4.5.11",
    "@vue/cli-plugin-eslint": "^4.5.11",
    "@vue/cli-plugin-typescript": "^4.5.11",
    "@vue/cli-plugin-unit-jest": "^4.5.11",
    "@vue/cli-plugin-vuex": "^4.5.11",
    "@vue/cli-service": "^4.5.11",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "^1.1.3",
    "@wmde/eslint-config-wikimedia-typescript": "^0.2.0",
    "cypress": "^6.3.0",
    "eslint": "^7.20.0",
    "eslint-config-wikimedia": "^0.17.0",
    "eslint-plugin-chai-friendly": "^0.6.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-vue": "^6.2.2",
    "geckodriver": "^1.22.1",
    "jest-axe": "^4.1.0",
    "netlify-cli": "^2.71.0",
    "npm-run-all": "^4.1.5",
    "postcss-dir-pseudo-class": "^5.0.0",
    "postcss-logical": "^4.0.2",
    "sass": "^1.32.7",
    "sass-loader": "^8.0.2",
    "start-server-and-test": "^1.12.0",
    "stylelint": "^13.10.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-use-logical": "^1.1.0",
    "typescript": "^3.9.9",
    "vue-template-compiler": "^2.6.11"
  },
  "_id": "@wmde/query-builder@1.0.0",
  "bugs": {
    "url": "https://phabricator.wikimedia.org/tag/wikidata_query_builder/"
  },
  "homepage": "https://github.com/wmde/query-builder#readme",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wmde/query-builder.git"
  }
}
